{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:wp": "nest build blog --webpack && nest build main --webpack",
    "build:watch": "concurrently \"nest build blog --webpack --webpackPath webpack-hmr.config.js --watch\"  \"nest build main --webpack --webpackPath webpack-hmr.config.js --watch\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "concurrently \"nest start blog\" \"nest start main\"",
    "start:debug": "nest start --debug --watch ",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "dev": "cross-env NODE_ENV=local concurrently \"nest start main  --watch\"  \"nest start blog  --watch\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.0",
