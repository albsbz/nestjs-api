    "rxjs": "^7.5.6",
    "sanitize-html": "^2.7.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.7",
    "@shelf/jest-mongodb": "^4.1.0",
    "@types/aws-lambda": "^8.10.103",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
