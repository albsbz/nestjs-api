    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    },
    "preset": "@shelf/jest-mongodb",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  }
}
